    "license": "MIT",

    "require": {
        "php": ">=5.6.20",
        "symfony/event-dispatcher": "~3.4",
        "symfony/yaml": "~3.4",
        "twig/twig": "~1.42",
        "pimple/pimple": "~3.2",
        "filp/whoops": "~2.3",
        "rockettheme/toolbox": "dev-develop",
        "scssphp/scssphp": "~1.0",
        "erusev/parsedown-extra": "~0.7"
    },
